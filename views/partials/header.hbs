<script>
 document.querySelector(".logo-svg").classList.remove("active")
      setTimeout(() => {
        document.querySelector(".logo-svg").classList.add("active")
    }, 500)
</script>

<script>
    let level1 = document.querySelector('.multilevel-dropdown .level-1');
    let level2 = document.querySelector('.multilevel-dropdown .level-2');
    function resetLevel2() {
        let level2 = document.querySelectorAll('.multilevel-dropdown .level-2');
        Array.from(level2).forEach(tag => {
            tag.style.display = "none";
        })
    }
    let level1Items = level1.querySelectorAll('li');

    Array.from(level1Items).forEach((liTag) => {
        liTag.onmouseover = () => {
            let height = level1.clientHeight;
            
            let liLevel2 = liTag.querySelector('.level-2');
            resetLevel2();
            if (liLevel2) {
                
                liLevel2.style.display = 'block';
                liLevel2.style['min-height'] = height + 'px';
            }
        };
    });
    function menuToggle() {
        const toggleMenu = document.querySelector(".menu");
        toggleMenu.classList.toggle("active");
      }
</script>