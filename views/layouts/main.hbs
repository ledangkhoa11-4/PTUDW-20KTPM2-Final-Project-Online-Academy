  <!--jquery chapter -->
  
    <script>
      $(".chapter_content").click(function () {
        var container = $(this).parents(".chapter");
        var answer = container.find(".lecture");
        var trigger = container.find(".icon_drop");

        answer.slideToggle(200);

        if (trigger.hasClass("effect_icon_drop")) {
          trigger.removeClass("effect_icon_drop");
        } else {
          trigger.addClass("effect_icon_drop");
        }
      });
    </script>
