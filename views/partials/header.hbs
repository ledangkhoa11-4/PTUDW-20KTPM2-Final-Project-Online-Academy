    .logo svg .svg-elem-1 {
        stroke-dashoffset: 3248.395751953125px;
        stroke-dasharray: 3248.395751953125px;
        fill: transparent;
        -webkit-transition: stroke-dashoffset 3s cubic-bezier(0.895, 0.03, 0.685, 0.22) 0s,
            fill 1.7s cubic-bezier(0.47, 0, 0.745, 0.715) 0.8s;
        transition: stroke-dashoffset 3s cubic-bezier(0.895, 0.03, 0.685, 0.22) 0s,
            fill 1.7s cubic-bezier(0.47, 0, 0.745, 0.715) 0.8s;
    }

    .logo svg.active .svg-elem-1 {
        stroke-dashoffset: 0;
        fill: rgb(115, 20, 20);
    }
